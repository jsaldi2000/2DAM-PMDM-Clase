<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="@dimen/padding"
    tools:context=".Activity5">

    <Button
        android:id="@+id/btnConLlave"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="conLlave"
        app:layout_constraintBottom_toTopOf="@id/btnSinLlave"
        android:text="@string/btn_conLlave" />

    <Button
        android:id="@+id/btnSinLlave"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="sinLlave"
        app:layout_constraintBottom_toBottomOf="parent"
        android:text="@string/btn_sinLlave" />

</androidx.constraintlayout.widget.ConstraintLayout>